# variable related...

#if miscellany_startsWith "/etc/path/to/file" "/home/"; then
#	echo 'yes'
#fi
miscellany_startsWith()
{
	[ "${1#${2}}" != "$1" ]
}
miscellany_doesNotStartWith()
{
	[ "${1#${2}}" = "$1" ]
}

#if miscellany_endsWith "/etc/path/to/file" "/file"; then
#	echo 'yes'
#fi
miscellany_endsWith()
{
	[ "${1%${2}}" != "$1" ]
}
miscellany_doesNotEndWith()
{
	[ "${1%${2}}" = "$1" ]
}
