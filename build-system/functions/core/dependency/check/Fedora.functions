core_dependency_check_Fedora_checkFunction()
{
	if [ -f /etc/fedora-release ]; then
		readonly core_dependency_distributionName="Fedora"
		return 0
	fi
	return 1
}
core_dependency_check_register core_dependency_check_Fedora_checkFunction

