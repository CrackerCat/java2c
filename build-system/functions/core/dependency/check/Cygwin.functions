dependency_check_Cygwin_checkFunction()
{
	if [ -x /bin/cygcheck.exe ]; then
		readonly dependency_distributionName="Cygwin"
		return 0
	fi
	return 1
}
dependency_check_register dependency_check_Cygwin_checkFunction

dependency_check_Cygwin_packageManager()
{
	global_exitError "Please run the Cygwin setup-x86.exe to install the packages $@"
}
