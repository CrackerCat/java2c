dependency_check_functions=''

dependency_check_register()
{
	if [ -z "$dependency_check_functions" ]; then
		dependency_check_functions="$1"
	else
		dependency_check_functions="$dependency_check_functions $1"
	fi
}

dependency_check_determineDistributionName()
{
	_dependency_check_determineDistributionName $dependency_check_functions
}
