core_dependency_check_functions=''

core_dependency_check_register()
{
	if [ -z "$core_dependency_check_functions" ]; then
		core_dependency_check_functions="$1"
	else
		core_dependency_check_functions="$core_dependency_check_functions $1"
	fi
}

core_uses_in core/dependency/check CentOS Cygwin Fedora Homebrew RedHat
